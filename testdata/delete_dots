-- stuff.in.go --
package stuff

func zzz() {
	foo()

	if err := x(); err != nil {
		panic(err)
	}

	bar()

	if err := y(); err != nil {
		panic(err)
	}

	baz()

	if err := y(); err != nil {
		// This should be deleted.
		panic(err)
	}

	qux()
}

-- stuff.out.go --
package stuff

func zzz() {
	bar()

	if err := y(); err != nil {
		panic(err)
	}

	baz()
	qux()
}


@@
-func name(foo string, bar int) (error, ...) {
+func name(foo string, bar int) (..., error) {
- return nil, ...
+ return ..., nil
}

-- unnamed.in.go --
package a

func name(foo string, bar int) (error, string) {
	return nil, "very valid go"
}

-- unnamed.out.go --
package a

func name(foo string, bar int) (string, error) {
	return "very valid go", nil
}


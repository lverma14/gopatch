-- foo.in.go --
package foo

type Bar struct {
	Name string
}

func (b *Bar) String() string {
	return b.Name
}

-- foo.out.go --
package foo

type Bar struct {
	Name string
}

func (b *Bar) String() string {
	if b == nil {
		return "<nil>"
	}
	return b.Name
}


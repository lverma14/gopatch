-- example.in.go --
package main

import (
	"fmt"
	"log"
	"time"
)

func main() {
	start := time.Now()

	if err := realLogic(); err != nil {
		log.Fatal(err)
	}

	fmt.Println("elapsed time:", time.Now().Sub(start))
}

-- example.out.go --
package main

import (
	"fmt"
	"log"
	"time"
)

func main() {
	start := time.Now()

	if err := realLogic(); err != nil {
		log.Fatal(err)
	}

	fmt.Println("elapsed time:", time.Since(start))
}

